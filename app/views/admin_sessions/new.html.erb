<div id="form_wrap">
  <h2>Admin Login</h2>
  <%= form_for(@admin_session) do |f| %>
  	<%= render :partial => "layouts/error_block", :locals => {:object => @admin_session} %>
    <p>
      <%= f.label :username %><br />
      <%= f.text_field :username %>
    </p
    <p>
      <%= f.label :password %><br />
      <%= f.password_field :password %>
    </p>
    <p class="checkbox">
      <%= f.check_box :remember_me %> <%= f.label :remember_me %>
    </p>
    <div class="button_field">
      <%= f.submit "Login", :class => "btn" %>
    </div>
  <% end %>
  <%= link_to 'Cancel', admin_sessions_path, :class => "cancel_link" %>
</div>


